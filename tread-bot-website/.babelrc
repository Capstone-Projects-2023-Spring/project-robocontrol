{
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-react",
      "@babel/preset-typescript" // Add this line
    ],
    "plugins": [
      [
        "transform-require-extensions",
        {
          "extensions": {
            ".md": "file-loader!./markdown-loader.js"
          }
        }
      ]
    ]
  }
  